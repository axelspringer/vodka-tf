{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Parameters": {
        "ProjectId": {
            "Type": "String",
            "Description": "AWS projectID used to associate new resources to team members"
        }
    },
    "Resources": {
        "HelloWorld": {
            "Type": "AWS::Lambda::Function",
            "Properties": {
                "Code": {
                    "S3Bucket": {
                        "Fn::ImportValue": {
                            "Fn::Join": ["-", [{
                                "Ref": "ProjectId"
                            }, {
                                "Ref": "AWS::Region"
                            }, "Bucket"]]
                        }
                    },
                    "S3Key": "60c8586aecf49761b5a720d31bea7d3c"
                },
                "Handler": "main",
                "Role": {
                    "Fn::ImportValue": {
                        "Fn::Join": ["-", [{
                            "Ref": "ProjectId"
                        }, {
                            "Ref": "AWS::Region"
                        }, "LambdaTrustRole"]]
                    }
                },
                "Runtime": "go1.x",
                "Tags": [{
                    "Value": "SAM",
                    "Key": "lambda:createdBy"
                }]
            }
        }
    }
}